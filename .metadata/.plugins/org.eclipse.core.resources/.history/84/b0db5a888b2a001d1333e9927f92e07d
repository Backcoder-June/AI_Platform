<script>
$(document).ready(function(){
	var mycanvas = $("#mycanvas")[0];
	var pencil = mycanvas.getContext("2d");	
	pencil.lineWidth = 3; 
	pencil.strokeStyle = "red"; 
	pencil.strokeRect(10,50,100,200);
	
});//ready

</script>

