<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>

<script src="js/jquery-3.6.0.min.js"></script>
<script>
$(document).ready(function(){
	var mycanvas = $("#mycanvas")[0];
	var pencil = mycanvas.getContext("2d");	

	var myimage = new Image(); 
	pencil.src = "http://localhost:8083/naverai/yuna1.png"; 
	pencil.onload = function(){
	pencil.drawImage(myimage,0,0,myimage.width, myimage.height);
	pencil.lineWidth = 3; 
	pencil.strokeStyle = "red"; 
	pencil.strokeRect(10,50,100,200);
	}
	
	
});//ready

</script>


</head>
<body>

<canvas id="mycanvas" width=500 height=500 style="border:2px solid blue">
</canvas>


</body>
</html>